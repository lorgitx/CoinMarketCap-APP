{
    "builds": [
        {
            "src": "API/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "client/views/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "API/server.js/$1"
          },
        {
            "src": "/(.*)",
            "dest": "client/views/$1"
        }
    ]
}