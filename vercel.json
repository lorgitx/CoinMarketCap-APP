{
    "builds": [
      {
        "src": "API/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/views/**/*",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/API/server.js"
      },
      {
        "source": "/((?!api).*)",
        "destination": "/client/views/$1"
      }
    ]
  }
  