{
    "version": 2,
    "builds": [
      {
        "src": "views/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "api/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }